    </BuildMacro>
    <BuildMacro Include="FREETYPE_INC">
      <Value>$(FREETYPE_INC)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="FREETYPE_LIB">
      <Value>$(FREETYPE_LIB)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="ANTTWEAKBAR">
      <Value>$(ANTTWEAKBAR)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="ANTTWEAKBAR_INC">
      <Value>$(ANTTWEAKBAR_INC)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="ANTTWEAKBAR_LIB">
      <Value>$(ANTTWEAKBAR_LIB)</Value>
